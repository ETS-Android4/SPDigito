<!DOCTYPE html>
<html>
  <%- include partials/head.ejs %>
  <body>
    <%- include partials/navbar.ejs %>
    <div class="container">
      <div class="form-container">
        <form class="border border-info" method="post" action="/savedata">
          <!-- Patient Info -->
          <%- include partials/patientcf.ejs %>

          <!-- Upload image -->
          <div class="result-icon float-right">
            <i
              class="fas fa-image"
              data-toggle="popover"
              data-placement="right"
              data-img="<%= imagefile %>"
            ></i>
          </div>

          <!-- Result -->
          <%- include partials/resultcf.ejs %>

          <!-- Button -->
          <% var edithref= '/edit'; %> <%- include partials/confirmformbtn.ejs%>
        </form>
      </div>
    </div>
  </body>
</html>
