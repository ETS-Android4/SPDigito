<div
  class="modal fade"
  id="cancelModal"
  tabindex="-1"
  role="dialog"
  aria-labelledby="cancelModalLabel"
  aria-hidden="true"
>
  <div class="modal-dialog modal-dialog-centered modal-sm" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="cancelModalLabel">Cancel Saving?</h5>
        <button
          type="button"
          class="close"
          data-dismiss="modal"
          aria-label="Close"
        >
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        Press 'OK' to cancel saving record.
      </div>
      <div class="modal-footer">
        <button
          type="button"
          class="btn btn-outline-secondary"
          data-dismiss="modal"
        >
          Cancel
        </button>
        <button
          type="button"
          onclick="location.href='/'"
          class="btn btn-danger"
        >
          OK
        </button>
      </div>
    </div>
  </div>
</div>
