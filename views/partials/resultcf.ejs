<div class="form-row">
  <div class="form-group col-sm-4 col-md-4 col-lg-4">
    <label for="sys">SYS</label>
    <input
      type="text"
      class="form-control"
      name="sys"
      value="<%= result.sys %>"
      readonly
    />
  </div>
  <div class="form-group col-sm-4 col-md-4 col-lg-4">
    <label for="dia">DIA</label>
    <input
      type="text"
      class="form-control"
      name="dia"
      value="<%= result.dia %>"
      readonly
    />
  </div>
  <div class="form-group col-sm-4 col-md-4 col-lg-4">
    <label for="pulse">PULSE</label>
    <input
      type="text"
      class="form-control"
      name="pulse"
      value="<%= result.pulse %>"
      readonly
    />
  </div>
</div>
